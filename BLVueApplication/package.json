{
  "name": "bl-vue2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build-application": "npm run --prefix ../core/bl-application build",
    "build-domain": "npm run --prefix ../core/bl-domain build",
    "build-shared": "npm run --prefix ../core/bl-shared build",
    "build-ui": "npm run --prefix ../bl-ui-core build",
    "build-managers": "npm run --prefix ../api-managers build",
    "build-custom-dependencies": "npm run build-shared && npm run build-domain && npm run build-application && npm run  build-managers && npm run build-ui",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "bl-ui-core": "file:../bl-ui-core",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}
